	width: 85%;
	margin-top: 100rpx;
	font-size: 36rpx;
}

.Text{
	position: static;
	width: 85%;
	margin-top: 25rpx;
	font-size: 32rpx;
}

.Mark{
	display: flex;
  	align-items: center;
  	justify-content: center;
	width: 85%;
	margin-top: 75rpx;
	font-size: 40rpx;
}

.backView{
	position:relative;
	width:100%;
	height:100%;
	background:rebeccapurple;
}

.blackImage{
	position: absolute;
	top:0px;
	bottom:0px;
	left:0px;
	width:100%;
	height: 100%;
	z-index: -999;
}

.bgImage{
background: url("image.png") no-repeat;
height:100%;
width:100%;
overflow: hidden;
background-size:cover;
}

.background {
  width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%;
  z-index: -1;
}